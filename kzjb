wait(1)
local args = {
    [1] = "臭屁虫",
    [2] = "All"
}

game:GetService("ReplicatedStorage").DefaultChatSystemChatEvents.SayMessageRequest:FireServer(unpack(args))
	end
end

wait(1)
local args = {
    [1] = "癞蛤蟆",
    [2] = "All"
}

game:GetService("ReplicatedStorage").DefaultChatSystemChatEvents.SayMessageRequest:FireServer(unpack(args))

Tab:AddLabel("roblox发言有限制，连续7条后要冷却10秒")

Tab:AddToggle({
	Name = "三字经（秒三）",
	Default = false,
	Callback = function(Value)
	_G.szj = Value
	    szj()		
	end    
})

