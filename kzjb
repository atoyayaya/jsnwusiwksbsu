local OrionLib = loadstring(game:HttpGet(('https://raw.githubusercontent.com/atoyayaya/wangzhan/main/xiba')))()

local Window = OrionLib:MakeWindow({Name = "扣字脚本", HidePremium = false, SaveConfig = true, ConfigFolder = "OrionTest"})

local Tab = Window:MakeTab({
	Name = "扣字脚本",
	Icon = "rbxassetid://4483345998",
	PremiumOnly = false
})

_G.szj = true 

function szj()
	while _G.szj == true do
	wait(1)
local args = {
    [1] = "行不行",
    [2] = "All"
}

game:GetService("ReplicatedStorage").DefaultChatSystemChatEvents.SayMessageRequest:FireServer(unpack(args))

wait(1)
local args = {
    [1] = "好不好",
    [2] = "All"
}

game:GetService("ReplicatedStorage").DefaultChatSystemChatEvents.SayMessageRequest:FireServer(unpack(args))

wait(1)
